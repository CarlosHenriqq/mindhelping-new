{
  "expo": {
    "name": "MindHelping",
    "slug": "MindHelping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "mindhelping",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5bf5c476-38cd-4173-8256-2072eb6e5c89"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindHelping",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.carloshenriqque.MindHelping",
      "versionCode": 1,
      "runtimeVersion": "1.0.0",
      "softwareKeyboardLayoutMode": "pan",
      "navigationBar": {
        "visible": false,
        "barStyle": "light-content",
        "backgroundColor": "#00000000"
      },
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "permissions": [
      "android.permission.CALL_PHONE"
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#2980B9",
      "androidMode": "default"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5bf5c476-38cd-4173-8256-2072eb6e5c89"
      }
    }
  }
}