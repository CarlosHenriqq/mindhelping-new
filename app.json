{
  "expo": {
    "name": "MindHelping",
    "slug": "MindHelping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "mindhelping",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindHelping",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.carloshenriqque.MindHelping"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "permissions": [
      "android.permission.CALL_PHONE"
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#2980B9",
      "androidMode": "default"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5bf5c476-38cd-4173-8256-2072eb6e5c89"
      }
    }
  }
}
